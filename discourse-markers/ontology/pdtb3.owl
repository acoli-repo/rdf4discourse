    <!-- https://catalog.ldc.upenn.edu/docs/LDC2019T05/PDTB3-Annotation-Manual.pdf#Arg1-as-subst -->

    <owl:Class rdf:about="https://catalog.ldc.upenn.edu/docs/LDC2019T05/PDTB3-Annotation-Manual.pdf#Arg1-as-subst">
