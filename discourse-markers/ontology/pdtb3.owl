    </owl:Class>
    


    <!-- https://catalog.ldc.upenn.edu/docs/LDC2019T05/PDTB3-Annotation-Manual.pdf#Arg2-as-denier -->

    <owl:Class rdf:about="https://catalog.ldc.upenn.edu/docs/LDC2019T05/PDTB3-Annotation-Manual.pdf#Arg2-as-denier">
        <rdfs:subClassOf rdf:resource="https://catalog.ldc.upenn.edu/docs/LDC2019T05/PDTB3-Annotation-Manual.pdf#Concession"/>
        <rdfs:comment xml:lang="en">The label Comparison.Concession.Arg2-as-denier is used when it is Arg1 that raises an expectation
