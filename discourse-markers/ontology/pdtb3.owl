    </owl:Class>
    


    <!-- https://catalog.ldc.upenn.edu/docs/LDC2019T05/PDTB3-Annotation-Manual.pdf#Arg2-as-cond -->

    <owl:Class rdf:about="https://catalog.ldc.upenn.edu/docs/LDC2019T05/PDTB3-Annotation-Manual.pdf#Arg2-as-cond">
        <rdfs:subClassOf rdf:resource="https://catalog.ldc.upenn.edu/docs/LDC2019T05/PDTB3-Annotation-Manual.pdf#Condition"/>
        <rdfs:comment xml:lang="en">The label Contingency.Condition.Arg2-as-cond is used when Arg2 describes the antecedent and
